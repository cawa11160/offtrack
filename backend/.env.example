# Optional (enables Spotify search + cover art)
SPOTIFY_CLIENT_ID=
SPOTIFY_CLIENT_SECRET=

# Postgres (local, outside docker)
DATABASE_URL=postgresql+psycopg2://postgres:postgres@localhost:5432/offtrack

# Comma-separated allowed origins for CORS (dev)
ALLOW_ORIGINS=http://localhost:8080,http://127.0.0.1:8080

# PostHog (optional analytics)
POSTHOG_ENABLED=true
POSTHOG_HOST=https://app.posthog.com
POSTHOG_API_KEY=
